{
  "name": "pipeline-profiles",
  "version": "1.3.4",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbradds/pipeline-profiles"
  },
  "license": "MIT",
  "author": "Grant Moss",
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-azure": "webpack --config webpack.azure.js",
    "deploy": "conda run -n pipeline-profiles python deploy/make_production_files.py",
    "analyze-build": "webpack --config webpack.analyze.js",
    "start": "node server.js",
    "heroku-postbuild": "webpack --config webpack.prod.js",
    "dev": "webpack serve --config webpack.dev.js",
    "update-traffic-data": "conda run -n pipeline-profiles python src/data_management/traffic.py",
    "update-incidents-data": "conda run -n pipeline-profiles python src/data_management/incidents.py",
    "update-conditions-data": "conda run -n pipeline-profiles python src/data_management/conditions.py",
    "update-apportionment-data": "conda run -n pipeline-profiles python src/data_management/apportionment.py",
    "update-oandm-data": "conda run -n pipeline-profiles python src/data_management/oandm.py",
    "update-all-data": "conda run -n pipeline-profiles python src/data_management/update_all.py",
    "get-maps": "conda run -n pipeline-profiles python src/data_management/get_map_files.py",
    "simplify-maps": "sh ./scripts/simplify-maps.sh",
    "test-backend": "conda run -n pipeline-profiles python src/data_management/tests.py",
    "test-frontend": "ava",
    "test-html5": "node test/html5.js",
    "save-conda-env": "sh ./scripts/save-environment.sh",
    "set-remote-work": "git remote set-url origin https://CER-REC@dev.azure.com/CER-REC/Pipeline%20Information/_git/pipeline-profiles",
    "set-remote-personal": "git remote set-url origin https://github.com/mbradds/pipeline-profiles.git",
    "prettier-save": "npx prettier --write .",
    "prettier-check": "npx prettier --check ."
  },
  "ava": {
    "extensions": {
      "js": true
    },
    "files": [
      "test/test.js"
    ],
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "require": [
      "esm"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.1",
    "haversine": "^1.1.1",
    "highcharts": "^9.3.1",
    "leaflet": "^1.7.1",
    "mapshaper": "^0.5.72"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "ava": "^3.15.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "core-js": "^3.19.1",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.1.4",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "html-validate": "^6.1.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "prettier": "2.4.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.64.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "os": [
    "linux",
    "win32"
  ]
}
