{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-nested-ternary": "off",
    "no-underscore-dangle": "warn",
    "new-cap": "warn",
    "no-param-reassign": ["error", { "props": false }],
    "import/extensions": [
      "error",
      {
        "js": "always",
        "json": "always"
      }
    ]
  }
}
